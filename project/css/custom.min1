  color: #e6ebef;
  line-height: 38px;
  border: 2px solid #e6ebef;/* and small *//* and large *//* Pop Button */
}
.btn.tertiary:hover, .btn.tertiary:active, .btn.tertiary.active {
  background: #e6ebef;
  color: white;
}
.btn.tertiary.small {
  line-height: 26px;
}
.btn.tertiary.large {
  line-height: 54px;
}
.btn.tertiary.pop-btn {
  transform: scale(1);
  box-shadow: 0 0 0 rgba(0, 0, 0, 0.1);
  transition: transform 100ms ease-in-out, background-color 200ms ease-in-out, box-sizing 500ms ease-in-out;
}
.btn.danger {
  background-color: #DF4744;
  color: #FFF;
}
/* Text buttons
/* -------------------------------------------------- */
.text-button {
  display: inline-block;
  /* margin: 0 $padding-base 0 0; */
  background: none;
  color: #0466d2;
  font-size: 13px;
  line-height: 1.2;
  text-align: center;
  text-decoration: none;
  border: none;
}
.text-button svg {
  position: relative;
  display: inline-block;
  width: 14px;
  height: 14px;
  top: 2px;
  margin-right: 3px;
}
.text-button:hover, .text-button:active {
  color: rgb(67, 140, 221);
  text-decoration: underline;
}
.text-button:disabled, .text-button.disabled {
  color: #494949;
  opacity: 0.7;
  text-decoration: line-through;
  cursor: text;
}
.text-button + .text-button {/* margin: 0 $padding-base 0 0; */
}
/* Button Bar
/* -------------------------------------------------- */
.button-bar {
  clear: both;
  display: -ms-flexbox;
  display: flex;
  position: relative;
  padding-top: 15px;
  background: white;
}
.button-bar .button, .button-bar .btn {
  -ms-flex: 1;
      flex: 1;
  margin-left: 15px
}
.button-bar .button:first-child, .button-bar .btn:first-child {
  margin-left: 0;
}
.button-bar .button.secondary, .button-bar .btn.secondary {
  -ms-flex: 0;
      flex: 0;
}
/* Buttons that 'POP'
/* -------------------------------------------------- */
.pop-btn {
  border: none;
  font-weight: bold;
  transform: scale(1);
  box-shadow: 0 0 0 rgba(0, 0, 0, 0.1);
  transition: transform 100ms ease-in-out, background-color 200ms ease-in-out, box-sizing 500ms ease-in-out;
}
.pop-btn:hover {
  transform: scale(1.07);
  box-shadow: 1px 5px 6px rgba(0, 0, 0, 0.3);
  text-shadow: 0px -1px 0px #999999;
  text-decoration: none;
  color: #fff;
}
.btn-25 {
  width: 24%;
}
