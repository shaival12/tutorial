/* -------------------------------------------------- */
/* Forms
/* -------------------------------------------------- */
.form-field {
  position: relative;
  margin-bottom: 15px;

  /* Primary Form Field Styles for extra focus */
}
.form-field > label, .form-field .form-label {
  position: relative;
  display: inline-block;
  margin: 0 0 5px;
  font-weight: bold;
}
.form-field .form-input {
  width: 100%;
}
.form-field.primary {
  padding: 6px;
  background: $gray-7;
  transition: padding .2s cubic-bezier(0.626, 0.000, 0.000, 1.000);
}
.form-field.primary .form-field, .form-field.primary .form-with-button {
  width: 100%;
  @include depth-1;
}
.form-field.primary .form-field .form-input, .form-field.primary .form-with-button .form-input {
  box-shadow: none;
}
.form-field.primary .form-input {
  width: auto;
  @include depth-1;
}
.form-desc {
  float: right;
  font-size: 12px;
}
h3 + .form-desc {
  margin-top: -36px;
}
.form-input {
  position: relative;
  height: 54px;
  padding: 10px 15px;
  margin: 0;
  background-color: white;
  background-image: none;
  font-size: 22px;
  color: #494949;
  line-height: 1.333;
  border: none;
  border-radius: 3px;
  box-shadow: inset 0 1px 3px 1px rgba(0, 0, 0, 0.25);
  outline: none;
  transition: all .2s cubic-bezier(0.626, 0.000, 0.000, 1.000);
}
/* Disabled and read-only inputs
     HTML5 says that controls under a fieldset > legend:first-child won't be
     disabled if the fieldset is disabled. Due to implementation difficulty, we
     don't honor that edge case; we style them as disabled anyway. */
.form-input[disabled], .form-input[readonly], fieldset[disabled] .form-input {
  background-color: #e6ebef;
  opacity: 1;
  box-shadow:0 0 0 transparent;
}
/* Unstyle the caret on `<select>`s in IE10+. */
.form-input[disabled], fieldset[disabled] .form-input {
  cursor: not-allowed;
}
.form-input::-ms-expand {
  border: 0;
  background-color: transparent;
}
.form-input:hover {
  background: #F8FBFE;
}
.form-input:focus {
  background: #F0F6FD;
}
.form-input.small {
  height: 29px;
  padding: 5px 8px;
  font-size: 15px;
}
.form-select {
  display: block;
  width: 100%;
  height: 48px;
  padding: 6px 12px;
  font-size: 14px;
  line-height: 1.42857143;
  color: #555555;
  background-color: #ffffff;
  background-image: none;
  border: 1px solid #cccccc;
  border-radius: 4px;
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
  transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
}
/* Radio and Checkboxes Input
/* -------------------------------------------------- */
.radio-input, .checkbox-input {
  position: relative;
  margin-bottom: 15px;
}
.radio-input .form-label, .checkbox-input .form-label {
  position: relative;
  display: inline-block;
  margin: 0 0 5px;
  font-size: 15px;
  font-weight: bold;
  line-height: 18px;
}
.radio-input label, .checkbox-input label {
  position: relative;
  display: block;
  min-height: 20px;
  padding-left: 24px;
  color: #494949;
  font-weight: 400;
  line-height: 1.25;
  cursor: pointer
}
.radio-input label + label, .checkbox-input label + label {
  margin-top: 3px;
}
.radio-input input[type="radio"], .radio-input input[type="checkbox"], .checkbox-input input[type="radio"], .checkbox-input input[type="checkbox"] {
  position: absolute;
  margin-left: -20px;
  margin-top: 2px;
}
.radio-input.disabled label, fieldset[disabled] .radio-input label, .checkbox-input.disabled label, fieldset[disabled] .checkbox-input label {
  cursor: not-allowed;
}
/* Apply same disabled cursor tweak as for inputs
   Some special care is needed because <label>s don't inherit their parent's `cursor`.
   Note: Neither radios nor checkboxes can be readonly. */
input[type="radio"][disabled], input[type="radio"].disabled, fieldset[disabled] input[type="radio"], input[type="checkbox"][disabled], input[type="checkbox"].disabled, fieldset[disabled] input[type="checkbox"] {
  cursor: not-allowed;
}
/* Textarea
/* -------------------------------------------------- */
textarea {
  display: block;
  position: relative;
  width: 100%;
  min-height: 140px;
  padding: 10px;
  background: none;
  font-size: 14px;
  line-height: 21px;
  border: none;
  outline: 0;
  resize: none;
  z-index: 1
}
textarea.form-input {
  font-size: 14px;
  line-height: 1.2;
}
/* Form Decorations
/* -------------------------------------------------- */
.form-inset {
  margin: 0 0 15px;
  padding: 15px;
  background-color: #eee;
  border: 1px solid #ddd;
  border-radius: 2px;
}
.form-inset label {
  font-weight: bold;
}
.form-field .error {
  position: relative;
  display: inline-block;
  padding: 5px;
  color: rgba(223, 71, 68, 0.8);
  font-size: 12px;
}
.form-tooltip {
  display: inline-block;
  margin: 0 0 0 5px;
  color: #0466d2;
  font-size: 12px;
  font-weight: bold;
}
.form-label .form-tooltip {
}
.inline-form-text {
  display: inline-block;
}
.alert {
  display: block;
  padding: 5px 0;
  margin: 0;
  color: #DF4744;
  font-size: 13px;
  list-style: none;
}
.alert li {
  padding: 0 0 3px;
}
.alert.alert-success {
  color: #149c37;
}
