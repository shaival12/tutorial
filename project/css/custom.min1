}
@media (min-width: 480px) {
  .btn.x-large {
    font-size: 36px;
  }
}
.btn.large {
  height: 58px;
  min-height: 58px;
  padding: 0 15px;
  font-size: 28px;
  font-weight: bold;
  line-height: 58px;
  border-radius: 6px;
  font-family: Roboto,Helvetica,Arial,sans-serif;
  font-weight: 500;
}
.btn.large:hover, .btn.large:focus {
  box-shadow: 1px 5px 6px rgba(0,0,0,.3);
}
.btn.small {
  height: 32px;
  min-height: 32px;
  padding: 0 10px;
  font-size: 14px;
  font-weight: bold;
  line-height: 30px;
}
.btn.x-small {
