/* Button with icon */
.btn .button-icon {
  position: relative;
  display: inline-block;
  top: 3px;
  width: 16px;
  height: 16px;
  margin-right: 4px;
  transition: all .2s cubic-bezier(0.626, 0.000, 0.000, 1.000);
}
.btn:hover, .btn:active, .btn.active {
  background: #F0F0F0;
  color: #494949;
  transform: scale(1.05);
}
.btn:disabled, .btn.disabled {
  opacity: 0.5;
}
.btn:disabled:hover, .btn.disabled:hover {
  transform: scale(1.0);
}
.btn.block {
  display: block;
}
.btn.block.small {
  line-height: 32px;
}
.btn.x-large {
  height: 84px;
  min-height: 84px;
  padding: 0 15px;
  font-size: 28px;
  font-weight: bold;
  line-height: 84px;
